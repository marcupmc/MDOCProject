<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="domain.PhoneNumber" table="PHONENUMBER">
        <id name="id" type="int">
            <column name="ID_PHONENUMBER" />
            <generator class="increment" />
        </id>
        <property name="phoneKind" type="java.lang.String">
            <column name="PHONEKIND" />
        </property>
        <property name="phoneNumber" type="java.lang.String">
            <column name="PHONENUMBER" />
        </property>
       
        <many-to-one name="contact" column="ID_CONTACT" class="domain.Contact"/>
    </class>
</hibernate-mapping>
